from collections import Counter

class Solution(object):
    def frequencySort(self, s):
        """
        :type s: str
        :rtype: str
        """
        if not s:
            return ""
        count = Counter(s)
        count_sorted = sorted(count.items(), key=lambda  x : x[1], reverse = True)
        output = []
        for chr in count_sorted:
            output.append(chr[0] * chr[1])
        return "".join(output)

